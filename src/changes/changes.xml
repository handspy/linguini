<document
        xmlns="http://maven.apache.org/changes/1.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
    <properties>
        <title>Project Changes</title>
        <author email="hello@josecpaiva.me">Jos√© Carlos Paiva</author>
    </properties>
    <body>
        <release version="1.0.0" date="2019-11-29" description="First release">
            <action dev="jcpaiva" type="update">
                First release of the library.
            </action>
        </release>
        <release version="1.0.1" date="2019-12-01" description="Lexical Diversity and JSpell fixes">
            <action dev="jcpaiva" type="update">
                Fix Lexical Diversity algorithms.
                Fix concurrency issues with JSpell.
            </action>
        </release>
        <release version="1.0.2" date="2019-12-02" description="Text analyzer updates">
            <action dev="jcpaiva" type="update">
                Update TextAnalyzer.
            </action>
        </release>
        <release version="1.0.3" date="2019-12-02" description="Log4j not a provided dependency">
            <action dev="jcpaiva" type="update">
                Make Log4j not a provided dependency.
            </action>
        </release>
        <release version="1.0.4" date="2019-12-02" description="JSpell dictionaries should be installed on filesystem">
            <action dev="jcpaiva" type="update">
                Use installed JSpell dictionaries should be installed on filesystem.
            </action>
        </release>
        <release version="1.0.5" date="2019-12-03" description="Fixes and improvements simple text analysis">
            <action dev="jcpaiva" type="update">
                Fix and improve simple text analysis.
            </action>
        </release>
        <release version="1.0.6" date="2019-12-03" description="Log4j to target folder">
            <action dev="jcpaiva" type="update">
                Log4j to target folder.
            </action>
        </release>
        <release version="1.0.7" date="2019-12-03" description="Log4j to catalina home logs folder">
            <action dev="jcpaiva" type="update">
                Log4j to catalina home logs folder.
            </action>
        </release>
        <release version="1.1.0" date="2020-01-03" description="Integrated Stanford NLP">
            <action dev="jcpaiva" type="update">
                Optimizations for Portuguese language tokenization, lemmatization, and classification (toponyms, abbreviations, clitics, contractions, augmentative/diminutive/superlative/number declensions, gender normalization, irregular verb conjugations, ...).
                PoS tagging with Stanford NLP.
                Neural Network Dependency Parsing with Stanford NLP.
                Co-occurrence analysis extended from GloVe.
                Idea Density based on IDD3 (Propositional Idea Density from Dependency Trees).
            </action>
        </release>
        <release version="1.1.1" date="2020-01-04" description="Fixes concurrency and idea density">
            <action dev="jcpaiva" type="update">
                Fixes concurrency and idea density.
            </action>
        </release>
    </body>
</document>
